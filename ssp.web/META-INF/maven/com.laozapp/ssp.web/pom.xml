		<artifactId>rtb</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
	<artifactId>ssp.web</artifactId>
	<packaging>war</packaging>
	<name>sspweb</name>
	<url>http://maven.apache.org</url>

	<dependencies>
		<dependency>
